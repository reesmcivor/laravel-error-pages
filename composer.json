{
    "name": "reesmcivor/laravel-error-pages",
    "description": "Custom error pages",
    "autoload": {
        "psr-4": {
            "ReesMcIvor\\LaravelErrorPages\\": "src/"
        }
    },
    "authors": [
        {
            "name": "reesmcivor",
            "email": "reesmcivor@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReesMcIvor\\LaravelErrorPages\\CustomErrorPagesServiceProvider"
            ],
            "config": {
                "laravel-error-pages": "src/config/laravel-error-pages.php"
            }
        }
    }
}
